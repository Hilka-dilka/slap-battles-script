local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")


local function showRobloxStyleNotification(title, message)
	local gui = Instance.new("ScreenGui")
	gui.Name = "RobloxStyleNotification"
	gui.IgnoreGuiInset = true
	gui.ResetOnSpawn = false
	gui.Parent = playerGui

	local frame = Instance.new("Frame")
	frame.AnchorPoint = Vector2.new(1, 1)
	frame.Position = UDim2.new(1, -20, 1, -20)
	frame.Size = UDim2.new(0, 300, 0, 80)
	frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
	frame.BorderSizePixel = 0
	frame.BackgroundTransparency = 0.1
	frame.ClipsDescendants = true
	frame.Visible = false
	frame.Parent = gui


	local icon = Instance.new("ImageLabel")
	icon.Size = UDim2.new(0, 60, 0, 60)
	icon.Position = UDim2.new(0, 10, 0.5, -30)
	icon.BackgroundTransparency = 1
	icon.Image = "rbxassetid://6031075938" 
	icon.ImageColor3 = Color3.fromRGB(255, 255, 255)
	icon.Parent = frame


	local titleLabel = Instance.new("TextLabel")
	titleLabel.BackgroundTransparency = 1
	titleLabel.Position = UDim2.new(0, 80, 0, 10)
	titleLabel.Size = UDim2.new(1, -90, 0, 25)
	titleLabel.Font = Enum.Font.GothamBold
	titleLabel.Text = title
	titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	titleLabel.TextSize = 18
	titleLabel.TextXAlignment = Enum.TextXAlignment.Left
	titleLabel.Parent = frame

	local messageLabel = Instance.new("TextLabel")
	messageLabel.BackgroundTransparency = 1
	messageLabel.Position = UDim2.new(0, 80, 0, 35)
	messageLabel.Size = UDim2.new(1, -90, 0, 30)
	messageLabel.Font = Enum.Font.Gotham
	messageLabel.Text = message
	messageLabel.TextColor3 = Color3.fromRGB(220, 220, 220)
	messageLabel.TextSize = 15
	messageLabel.TextXAlignment = Enum.TextXAlignment.Left
	messageLabel.Parent = frame


	frame.Visible = true
	frame.Position = UDim2.new(1, 320, 1, -20)
	TweenService:Create(frame, TweenInfo.new(0.6, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
		Position = UDim2.new(1, -20, 1, -20)
	}):Play()


	task.wait(5)


	TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.In), {
		Position = UDim2.new(1, 320, 1, -20)
	}):Play()
	task.wait(0.6)
	gui:Destroy()
end


showRobloxStyleNotification("Created by: Alargon", "Have fun!")



local character = player.Character or player.CharacterAdded:Wait()
local camera = workspace.CurrentCamera

player.CameraMaxZoomDistance = 20
player.CameraMinZoomDistance = 15

local function teleportToCFrame(cf)
	character:PivotTo(cf)
end

local function teleportToObject(object)
	if object and object:IsA("BasePart") then
		character:PivotTo(object.CFrame)
	else
		warn("Object not found or not a BasePart:", object)
	end
end

local function waitForPlayerTrigger(prompt)
	if not prompt or not prompt:IsA("ProximityPrompt") then
		warn("ProximityPrompt not found!")
		return
	end
	local triggered = false
	local connection
	connection = prompt.Triggered:Connect(function(plr)
		if plr == player then
			triggered = true
		end
	end)
	repeat task.wait() until triggered
	connection:Disconnect()
end

for i = 1, 5 do
	local startCFrame = CFrame.new(
		-1822.81714, -69.036499, 309.295258,
		-0.995781124, -4.41443433e-08, 0.0917604938,
		-4.05582163e-08, 1, 4.09461336e-08,
		-0.0917604938, 3.70517448e-08, -0.995781124
	)
	teleportToCFrame(startCFrame)
	local prompt1 = workspace.Shed.StartDoor["Cylinder.025"].Attachment:FindFirstChildOfClass("ProximityPrompt")
	waitForPlayerTrigger(prompt1)

	if i <= 4 then
		task.wait(8)
		local neonExitSign = workspace.Maze.transition.visual:WaitForChild("Neon Exit Sign")
		local children = neonExitSign:GetChildren()
		local targetPart = children[2]
		if targetPart and targetPart:IsA("BasePart") then
			teleportToObject(targetPart)
		else
			warn("Second child of Neon Exit Sign is not a BasePart")
		end
		local prompt2 = workspace.Maze.transition.visual.Shed.Door["Cylinder.025"].EndPromptAttachment:FindFirstChild("EndPrompt")
		waitForPlayerTrigger(prompt2)
		task.wait(8)
	else
		task.wait(16)
		local harvesterHandle = workspace:WaitForChild("Maze"):WaitForChild("BarnMaze"):WaitForChild("CoreParts"):WaitForChild("Harvester"):WaitForChild("Handle")
		teleportToObject(harvesterHandle)
		task.wait(18)
        print("ðŸš¨ Teleporting to ChaseZones['15'] now!")
		local chaseZone = workspace:WaitForChild("Maze"):WaitForChild("BarnMaze"):WaitForChild("ChaseZones"):WaitForChild("15")
		teleportToObject(chaseZone)
	end
end

print("All 5 passes completed")
